  --label traefik.http.routers.amaliriel.rule=Host\(\`amaliriel.com\`\) \
  --label traefik.http.routers.amaliriel.tls.certresolver=lets-encrypt \
  --label traefik.http.routers.amaliriel.tls=true \
  --label traefik.http.routers.amaliriel.service=amaliriel-credit-http \
  --label traefik.http.services.amaliriel-credit-http.loadbalancer.server.port=80 \
  --name amaliriel \
  --network web \
  --restart always \
  --expose 80 \
