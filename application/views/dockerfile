docker run -it -d \
  --label traefik.http.routers.bagarapharmacy.rule=Host\(\`bagarapharmacy.phamasoft.com\`\) \
  --label traefik.http.routers.bagarapharmacy.tls.certresolver=lets-encrypt \
  --label traefik.http.routers.bagarapharmacy.tls=true \
  --label traefik.http.routers.bagarapharmacy.service=bagarapharmacy-credit-http \
  --label traefik.http.services.bagarapharmacy-credit-http.loadbalancer.server.port=80 \
  --name bagarapharmacy \
  --network web \
  --restart always \
  --expose 80 \
